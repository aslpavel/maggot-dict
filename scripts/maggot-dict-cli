# maggot-dict-cli bash completion
#   maggot-dict-cli checks environment for COMP_LINE and COMP_POINT
#   variables and ,if they are found prefroms compltion instead of lookup
##
alias d='maggot-dict-cli'
complete -C maggot-dict-cli maggot-dict-cli
complete -C maggot-dict-cli d

